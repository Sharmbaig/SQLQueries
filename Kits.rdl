  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Kits</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startdate">
            <Value>=Parameters!startdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@enddate">
            <Value>=Parameters!enddate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	a.order_id,
	cast(a.date_ordered as date) as date_ordered,
	a.item_id
FROM
(SELECT
	a.order_id,
	a.date_ordered,
	b.item_id
FROM
[PH_PROD].[dbxx].[hi_orders] as a
		INNER JOIN
[PH_PROD].[dbxx].[hi_order_lines] as b
		on a.order_id = b.order_id
WHERE b.item_id in ('KT1023', 'KT1022', 'KT1023SP', 'KT1023PR', 'KT1022SP', 'KT1022PR') and cast(a.date_ordered as date) between @startdate and @enddate
	
UNION

SELECT
	a.order_id,
	a.date_ordered,
	b.item_id
FROM
[PH_PROD].[dbxx].[orders] as a
	INNER JOIN
[PH_PROD].[dbxx].[order_lines] as b
	on a. order_id = b.order_id
WHERE cast(a.date_ordered as date) between @startdate and @enddate and b.item_id in ('KT1023', 'KT1022', 'KT1023SP', 'KT1023PR', 'KT1022SP', 'KT1022PR')) as a</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
